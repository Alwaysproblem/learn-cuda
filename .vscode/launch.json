{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "CUDA C++: Launch",
      "type": "cuda-gdb",
      "request": "launch",
      "program": "${command:cmake.launchTargetPath}"
    },
    {
      "type": "lldb-dap",
      "request": "launch",
      "name": "lldb dap Launch",
      "program": "${command:cmake.launchTargetPath}",
      "args": [],
      "env": [],
      "cwd": "${workspaceRoot}"
    },
    {
      "name": "cpp Launch",
      "type": "cppdbg",
      "request": "launch",
      // Resolved by CMake Tools:
      "program": "${command:cmake.launchTargetPath}",
      "args": ["1", "5"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [
        {
          // add the directory where our target was built to the PATHs
          // it gets resolved by CMake Tools:
          "name": "PATH",
          "value": "${env:PATH}:${command:cmake.getLaunchTargetDirectory}"
        }
      ],
      // "logging": {
      //   "trace": true,
      //   "traceResponse": true,
      //   "engineLogging": true
      // },
      "externalConsole": false,
      "linux": {
        // Resolved by CMake Tools:
        "program": "${command:cmake.launchTargetPath}",

        // For Ctest
        // "cwd": "${cmake.testWorkingDirectory}",
        // "program": "${cmake.testProgram}",
        // "args": [ "${cmake.testArgs}"],

        "MIMode": "gdb",
        "miDebuggerPath": "/usr/bin/gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ]
      },
      "osx": {
        // Resolved by CMake Tools:
        "program": "${command:cmake.launchTargetPath}",
        // For Ctest
        // "cwd": "${cmake.testWorkingDirectory}",
        // "program": "${cmake.testProgram}",
        // "args": [ "${cmake.testArgs}"],
        "externalConsole": false,
        "MIMode": "lldb"
      },
      "windows": {
        // Resolved by CMake Tools:
        "program": "${command:cmake.launchTargetPath}",
        // For Ctest
        // "cwd": "${cmake.testWorkingDirectory}",
        // "program": "${cmake.testProgram}",
        // "args": [ "${cmake.testArgs}"],
        "MIMode": "gdb",
        "miDebuggerPath": "C:\\MinGw\\bin\\gdb.exe"
      }
    }
  ]
}
